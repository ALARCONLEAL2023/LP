<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="ManagerBaseConfiguration" type="Tenaris.Manager.Forum.Shared.Configuration.ManagerBaseConfiguration, Tenaris.Manager.Forum.Shared"/>
    <section name="LinkerCTManager" type="Tenaris.Library.Framework.Factory.FactoryConfiguration, Tenaris.Library.Framework" />
    <section name="log" type="Tenaris.Library.Log.LogConfiguration, Tenaris.Library.Log"/>
  </configSections>


  <LinkerCTManager
    factoryType="Tenaris.Manager.ProcessExecutor.ProcessExecutorManagerFactory, Tenaris.Manager.ProcessExecutor"
    isSingleton="True">
    <ProcessExecutorManagerConfiguration
      managerName="LinkerCTManager"
      filePath="Tenaris.Manager.LinkerCT.Service.exe"
      childKeepAlive="true"
      childSocketAddress="localhost:0" />
  </LinkerCTManager>

  <log
    maxFileSize="8096"
    msgFormat="{Timestamp:HH:mm:ss.fff} ; {Level,-7} ; {ClassName} ; {Message} ; {MethodName}"
    path="..\..\..\Logs\Manager\LinkerCTManager\"
    logLevel="Debug"
	logBaseName="LinkerCTManager"
    traceAttributeLogEnabled="false"
    additionalLogDestination="Console"
    />

  <system.runtime.remoting>
    <customErrors mode="off" />
    <application name="LinkerCTManager">
      <channels>
        <channel ref="tcp" port="50031" >
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
          <clientProviders>
            <provider type="Tenaris.Library.Proxy.Remoting.ClientRemotingInterceptorProvider, Tenaris.Library.Proxy"/>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </clientProviders>
        </channel>
      </channels>
      <service>
      </service>
    </application>
  </system.runtime.remoting>

 <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Tenaris.Library.Log" publicKeyToken="bbb03b5ff4bbb68b" culture="neutral" />
        <codeBase version="4.1.0.0" href="v1\Tenaris.Library.Log.dll" />
      </dependentAssembly>
	        <dependentAssembly>
        <assemblyIdentity name="Tenaris.Library.DbClient" publicKeyToken="c58e4ef934a092e9" culture="neutral" />
        <codeBase version="4.1.0.0" href="v1\Tenaris.Library.DbClient.dll" />
      </dependentAssembly>
	    <dependentAssembly>
        <assemblyIdentity name="Tenaris.Library.ConnectionMonitor" publicKeyToken="69f947a774df559b" culture="neutral" />
        <codeBase version="4.1.0.0" href="v1\Tenaris.Library.ConnectionMonitor.dll" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

</configuration>
