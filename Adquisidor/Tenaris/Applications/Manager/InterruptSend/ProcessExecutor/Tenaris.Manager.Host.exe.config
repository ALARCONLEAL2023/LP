<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="ManagerBaseConfiguration" type="Tenaris.Manager.Forum.Shared.Configuration.ManagerBaseConfiguration, Tenaris.Manager.Forum.Shared"/>
    <section name="ProcessExecutor_SendInterrupt" type="Tenaris.Library.Framework.Factory.FactoryConfiguration, Tenaris.Library.Framework" />

    <section name="log" type="Tenaris.Library.Log.LogConfiguration, Tenaris.Library.Log"/>
  </configSections>


  <ProcessExecutor_SendInterrupt
    factoryType="Tenaris.Manager.ProcessExecutor.ProcessExecutorManagerFactory, Tenaris.Manager.ProcessExecutor"
    isSingleton="True">
    <ProcessExecutorManagerConfiguration
      managerName="InterruptSendManager"
      filePath="..\Tamsa.Common.Service.ITPush.Host.exe"
      childKeepAlive="true"
      childSocketAddress="localhost:33333" />
  </ProcessExecutor_SendInterrupt>

<!--
  <Tenaris.Manager.ProcessExecutor
    factoryType="Tenaris.Manager.ProcessExecutor.ProcessExecutorManagerFactory, Tenaris.Manager.ProcessExecutor"
    isSingleton="True">
    <ProcessExecutorManagerConfiguration
      managerName="InterruptSendManager"
      filePath="C:\Windows\notepad.exe"
      childType=""
      childUrl=""
      childSocketAddress="localhost:33333" />
  </Tenaris.Manager.ProcessExecutor>
-->

  <log
    maxFileSize="8096"
    msgFormat="{Timestamp:HH:mm:ss.fff} ; {Level,-7} ; {ClassName} ; {Message} ; {MethodName}"
    path="..\..\..\..\Logs\Manager\InterruptSend\"
    logLevel="Debug"
    traceAttributeLogEnabled="false"
    additionalLogDestination="Console"
    logBaseName="ProcessExecutor"
    />

  <system.runtime.remoting>
    <customErrors mode="off" />
    <application name="InterruptSendManager">
      <channels>
        <channel ref="tcp" port="33334" bindTo="127.0.0.1">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
          <clientProviders>
            <provider type="Tenaris.Library.Proxy.Remoting.ClientRemotingInterceptorProvider, Tenaris.Library.Proxy"/>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </clientProviders>
        </channel>
      </channels>
      <service>
      </service>
    </application>
  </system.runtime.remoting>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

</configuration>
