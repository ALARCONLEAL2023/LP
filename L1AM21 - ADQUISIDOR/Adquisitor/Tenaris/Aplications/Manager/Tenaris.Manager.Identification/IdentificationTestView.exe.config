<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!-- esta seccion debe ser la primera antes de todo -->
  <configSections>
    <!-- configuracion de la vista -->
    <section name="TemplateConfig" type="IdentificationTestView.Configuration.IdentificationTestViewConfiguration, IdentificationTestView" />

    <section name="Tenaris.Manager.Identification.IdentificationManager"
             type="Tenaris.Library.Proxy.Factories.GenericProxyFactoryConfigurationSection, Tenaris.Library.Proxy" />

    <section name="log"
             type="Tenaris.Library.Log.LogConfiguration, Tenaris.Library.Log" />
  </configSections>

  <connectionStrings>
    <add name="Level2" connectionString="Application Name=TemplateView;Data Source=127.1.1.1;Initial Catalog=AusV1;User ID=sa;Password=Lineas123;Persist Security Info=True;MultipleActiveResultSets=True; Asynchronous Processing=true;"/>
  </connectionStrings>

  <Tenaris.Manager.Identification.IdentificationManager instanceType="Tamsa.Manager.Identification.Shared.Contract.ITenaris.Manager.Identification.IdentificationManager, Tamsa.Manager.Identification.Shared"
                  aliveInterval="5000"
                  singleInstance="true"
                  validateConnection="false">
    <Redundancy>
      <Address url="tcp://10.92.70.84:50020/Tenaris.Manager.Identification.IdentificationManager.soap" forumPresent="false" isTargetManager="true" />
    </Redundancy>
  </Tenaris.Manager.Identification.IdentificationManager>

  <!--ConfiguraciÃ³n -->
  <TemplateConfig DskMgr="false"
                  Language="es-MX"
                  DBConnection="Level2"
                  AreaCode="LINEA1"
                  MachineCode="L1_AMA21"
                  />


  <!--Remoting configuration. This can be placed in a separate file named remoting.config-->
  <system.runtime.remoting>
    <application name="Client">
      <channels>
        <channel ref="tcp" port="0">
          <clientProviders>
            <provider type="Tenaris.Library.Proxy.Remoting.ClientRemotingInterceptorProvider, Tenaris.Library.Proxy" />
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="bin;"/>
    </assemblyBinding>
  </runtime>

  <log
      maxFileSize="8192"
      msgFormat="{Timestamp:HH:mm.ss.fff} ; {Level,-7} ; {Message} ; {MethodName}"
      logLevel="Debug"
      traceAttributeLogEnabled="false"
      logBaseName="GenericManagerTest"
      additionalLogDestination="Console" />

</configuration>